<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>KnifeZ's .NET5 Frameowrk</Description>
    <Version>0.0.3</Version>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://gitee.com/KnifeZ/KnifeZ.Virgo</RepositoryUrl>
    <PackageTags>knifez,mvvm,.net5</PackageTags>
    <NeutralLanguage>zh-CN</NeutralLanguage>
    <Authors>KnifeZ</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Areas\templates\**" />
    <Compile Remove="Controllers\**" />
    <Compile Remove="GeneratorFiles\**" />
    <EmbeddedResource Remove="Areas\templates\**" />
    <EmbeddedResource Remove="Controllers\**" />
    <EmbeddedResource Remove="GeneratorFiles\**" />
    <None Remove="Areas\templates\**" />
    <None Remove="Controllers\**" />
    <None Remove="GeneratorFiles\**" />
    <None Include="C:\Users\Knife\Pictures\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="_CodeGenController.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.SpaServices.Extensions" Version="5.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KnifeZ.Virgo.Core\KnifeZ.Virgo.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Areas\" />
  </ItemGroup>

</Project>
